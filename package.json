{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce application built with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "node --watch backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seederScript.js",
    "data:destroy": "node backend/seederScript.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benmhara/e-commerce-proshop.git"
  },
  "author": "Mohamed Malek Ben Mhara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benmhara/e-commerce-proshop/issues"
  },
  "homepage": "https://github.com/benmhara/e-commerce-proshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1"
  }
}
